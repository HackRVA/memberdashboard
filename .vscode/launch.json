{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "FAKE_USER": "true"
      }
    },
    {
      "name": "Debug current file",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${file}",
      "args": ["${input:args}"]
    },
    {
      "name": "Test Current File",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "program": "${file}",
      "env": {},
      "args": [],
      "showLog": true
  }
  ],
  "inputs": [
    {
      "id": "args",
      "description": "Arguments",
      "type": "promptString"
    }
  ]
}
